<tr id="saved_weather_<%= saved_weather.id %>">
  <td><%= saved_weather.yahoo_title %></td>
  <td><%= saved_weather.yahoo_text %></td>
  <td><%= saved_weather.yahoo_temp %> degrees</td>
  <td><%= saved_weather.owm_cloud %></td>
  <td><%= saved_weather.owm_min %></td>
  <td><%= saved_weather.owm_max %></td>
  <td>
    <%= link_to 'Destroy', saved_weather, class: :delete_weather,
      method: :delete, 'data-confirm': 'Are you sure?', remote: true %>
  </td>
</tr>
